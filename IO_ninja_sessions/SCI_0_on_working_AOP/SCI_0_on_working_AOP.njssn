[session]
main-plugin-id = serialmon
log-file = SCI_0_on_working_AOP.njlog
document-settings-file = SCI_0_on_working_AOP.njcfg

[main-plugin]
portName = "COM11"
pendingNotifySizeLimit = 1048576
readParallelism = 4
readBlockSize = 4096
readBufferSize = 16384
notifyReadBufferFull = 0
[log-filter]
serial-setting = 1
serial-control = 1
serial-status = 1
serial-line-error = 1
